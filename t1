#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#define TAM 5
int* Soma(int N1, int N2, int D1, int D2){
    int Num, Den, Vet[2];
    Num=N1*D2+N2*D1;
	Den=D1*D2; 
	Vet[0]=Num;
	Vet[1]=Den;
	return Vet;
}
int* Subtrai(int N1, int N2, int D1, int D2){
    int Num,Den, Vet[2];
    Num=N1*D2-N2*D1;
    Den=D1*D2;
    Vet[0]=Num;
	Vet[1]=Den;
	return Vet;
}
int* Multiplicacao(int N1, int N2, int D1, int D2){
    int Num,Den, Vet[2];
    Num=N1*N2;
    Den=D1*D2;
    Vet[0]=Num;
	Vet[1]=Den;
	return Vet;
}
int* Divisao(int N1, int N2, int D1, int D2){
    int Num,Den, Vet[2];
    Num=N1*D2;
    Den=N2*D1;
    Vet[0]=Num;
	Vet[1]=Den;
	return Vet;
}
int MDC(int x, int y) {
    if (x < y) {
        int troca = y;
        y = x;
        x = troca;
    }
    if (x % y == 0)
        return y;
    return MDC(y, x % y);
}
int main(){
    int Teste, X1, X2, Y1, Y2,i,*Vet,Aux1, Aux2;
    char Op,Op2,Op3;
    scanf("%d",&Teste); 
	if(Teste >= 1 && Teste <= 10000){
	    while(i<Teste){
		    scanf("%d%s%d%s%d%s%d", &X1,&Op2,&Y1,&Op,&X2,&Op3,&Y2);//2ยบ
	        if(X1>= 1 && X1 <= 1000 &&
	        	X2>= 1 && X2 <= 1000 &&
	        	Y1>= 1 && Y1 <= 1000 &&
	        	Y2>= 1 && Y2 <= 1000)
				{
	    			switch(Op){
	    				case '-':
	    					Vet = Subtrai(X1,X2,Y1,Y2);
	    					Aux1 = Vet[0];
	    					Aux2 = Vet[1];
	    					printf("%d / %d = %d / %d\n", Aux1,Aux2, Aux1/(MDC((Aux1<0?-Aux1:Aux1),Aux2)), Aux2/(MDC((Aux1<0?-Aux1:Aux1),Aux2)));
	    					break;
	    				case '+':
	    					Vet = Soma(X1,X2,Y1,Y2);
	    					Aux1 = Vet[0];
	    					Aux2 = Vet[1];
	    					printf("%d / %d = %d / %d\n", Aux1,Aux2, Aux1/(MDC((Aux1<0?-Aux1:Aux1),Aux2)), Aux2/(MDC((Aux1<0?-Aux1:Aux1),Aux2)));
	    					break;
	    				case '*':
	    					Vet = Multiplicacao(X1,X2,Y1,Y2);
	    					Aux1 = Vet[0];
	    					Aux2 = Vet[1];
	    					printf("%d / %d = %d / %d\n", Aux1,Aux2, Aux1/(MDC((Aux1<0?-Aux1:Aux1),Aux2)), Aux2/(MDC((Aux1<0?-Aux1:Aux1),Aux2)));
	    					break;
	    				case '/':
	    					Vet = Divisao(X1,X2,Y1,Y2);
	    					Aux1 = Vet[0];
	    					Aux2 = Vet[1];
	    					printf("%d / %d = %d / %d\n", Aux1,Aux2, Aux1/(MDC((Aux1<0?-Aux1:Aux1),Aux2)), Aux2/(MDC((Aux1<0?-Aux1:Aux1),Aux2)));
	    					break;
					}
				}
		i++; 
    }
    
	}

}
